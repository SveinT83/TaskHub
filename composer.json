{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ezyang/htmlpurifier": "^4.17",
        "guzzlehttp/guzzle": "^7.9",
        "laravel/framework": "^11.9",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.16",
        "laravel/tinker": "^2.9",
        "livewire/livewire": "^3.5",
        "spatie/laravel-permission": "^6.9",
        "tronderdata/td-categories": "dev-main",
        "tronderdata/td-kb-articles": "dev-main",
        "tronderdata/td-clients": "dev-main",
        "tronderdata/td-task": "dev-main",
        "tronderdata/td-tickets": "dev-main",
    "tronderdata/td-equipment": "dev-main",
    "tronderdata/td-cost-calcultaror": "*"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/breeze": "^2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./modules/td-clients"
        },
        {
            "type": "path",
            "url": "./modules/td-task"
        },
        {
            "type": "path",
            "url": "./modules/td-tickets"
        },
        {
            "type": "path",
            "url": "./modules/td-kb-articles"
        },
        {
            "type": "path",
            "url": "./modules/td-categories"
        },
        {
            "type": "path",
            "url": "./modules/td-equipment"
        },
        {
            "type": "path",
            "url": "./modules/td-cost-calcultaror"
        },
        {
            "type": "path",
            "url": "./modules/td-sla"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "TronderData\\Task\\": "modules/td-task/src/",
            "TronderData\\KbArticles\\": "modules/td-kb-articles/src/",
            "TronderData\\Categories\\": "modules/td-categories/src/",
            "TronderData\\Clients\\": "modules/td-clients/src/",
            "TronderData\\Equipment\\": "modules/td-equipment/src/",
            "TronderData\\TdCostCalcultaror\\": "modules/td-cost-calcultaror/src/",
            "TronderData\\Sla\\": "modules/td-sla/src/"
        },
        "files": [
            "app/Helpers/SettingHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "TronderData\\Clients\\Providers\\ClientsServiceProvider",
                "TronderData\\Task\\Providers\\TdTaskServiceProvider",
                "TronderData\\KbArticles\\Providers\\KbArticlesServiceProvider",
                "TronderData\\Categories\\Providers\\CategoriesServiceProvider",
                "TronderData\\Equipment\\Providers\\EquipmentServiceProvider",
                "TronderData\\TdCostCalcultaror\\Providers\\TdCostCalcultarorServiceProvider",
                "TronderData\\Sla\\Providers\\SlaServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
