{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ezyang/htmlpurifier": "^4.17",
        "guzzlehttp/guzzle": "^7.9",
        "laravel/framework": "^11.9",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.16",
        "laravel/tinker": "^2.9",
        "livewire/livewire": "^3.5",
        "spatie/laravel-permission": "^6.9",
        "facebook/php-business-sdk": "^21.0",
        "taskhub/facebook-posting-module": "dev-Plov94-branch",
        "taskhub/credentials-bank": "dev-Plov94-branch"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/breeze": "^2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1"
    },

    "repositories": [
        {
            "type": "path",
            "url": "./Modules/Projects"
        },
        {
            "type": "path",
            "url": "./Modules/Customers"
        },
        {
            "type": "path",
            "url": "./Modules/Inventory"
        },
        {
            "type": "path",
            "url": "./Modules/AuditLogs"
        },
        {
            "type": "path",
            "url": "./Modules/Invoicing"
        },
        {
            "type": "path",
            "url": "./Modules/FacebookPostingModule"
        },
        {
            "type": "path",
            "url": "./Modules/CredentialsBank"
        }
    ],

    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\Projects\\": "Modules/Projects/src/",
            "Modules\\Customers\\": "Modules/Customers/src/",
            "Modules\\Inventory\\": "Modules/Inventory/src/",
            "Modules\\AuditLogs\\": "Modules/AuditLogs/src/",
            "Modules\\Invoicing\\": "Modules/Invoicing/src/",
            "Modules\\FacebookPostingModule\\": "Modules/FacebookPostingModule/src/",
            "Modules\\CredentialsBank\\": "Modules/CredentialsBank/src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },

    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },

    "extra": {
        "laravel": {
            "providers": [
                "Modules\\Projects\\src\\Providers\\ProjectServiceProvider",
                "Modules\\Customers\\src\\Providers\\CustomerServiceProvider",
                "Modules\\Inventory\\src\\Providers\\InventoryServiceProvider",
                "Modules\\AuditLogs\\src\\Providers\\AuditLogServiceProvider",
                "Modules\\Invoicing\\src\\Providers\\InvoicingServiceProvider",
                "Modules\\FacebookPostingModule\\src\\Providers\\FacebookPosterServiceProvider",
                "Modules\\CredentialsBank\\src\\Providers\\CredentialsBankServiceProvider"

            ]
        }
    },

    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}