    "type": "library",
    "autoload": {
        "psr-4": {
            "Modules\\CredentialsBank\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\CredentialsBank\\Providers\\CredentialsBankServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
