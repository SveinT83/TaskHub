{
    "name": "facebook/facebook-posting",
    "description": "A module for posting to Facebook groups",
    "type": "library",
    "autoload": {
      "psr-4": {
        "Modules\\FacebookPostingModule\\": "src/"
      },
      "files": [
        "src/Helpers/helpers.php"
      ],
      "exclude-from-classmap": [
        "vendor/"
      ]
    },
    "autoload-dev": {
      "psr-4": {
        "Modules\\FacebookPostingModule\\Tests\\": "tests/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Modules\\FacebookPostingModule\\Providers\\FacebookPosterServiceProvider"
        ],
        "aliases": {
          "FacebookApi": "Modules\\FacebookPostingModule\\Services\\FacebookApiService"
        }
      }
    },
    "require": {
      "php": "^7.3",
      "facebook/graph-sdk": "^5.7",
      "illuminate/support": "^8.0",
      "illuminate/database": "^8.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^9.3",
      "fakerphp/faker": "^1.9",
      "mockery/mockery": "^1.4"
    }
  }
