{
    "name": "modules/facebookposter",
    "description": "A module built using the facebook sdk that allows for posting to facebook using taskhub",
    "type": "module",
    "autoload": {
        "psr-4": {
            "Modules\\FacebookPostingModule\\": "src/"
        }
    },
    "require": {
        "facebook/graph-sdk": "^5.1",
        "laravel/framework": "^11.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\FacebookPostingModule\\src\\Providers\\FacebookPosterServiceProvider"
            ]
        }
    }
}
